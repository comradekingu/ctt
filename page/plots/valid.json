{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "title": "number of keys valid per day",
    "width": 550,
    "height": 250,
    "data": {
        "url": "./plots/data.json"
    },
    "mark": {
        "type": "line",
        "point": true
    },
    "encoding": {
        "x": {
            "timeUnit": "utcmonthdate",
            "field": "date",
            "type": "temporal",
            "axis": {
                "labelAngle": 0,
                "tickCount": "day"
            }
        },
        "y": {
            "field": "valid",
            "type": "quantitative"
        }
    }
}
