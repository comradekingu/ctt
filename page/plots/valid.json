{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "title": "number of keys valid per day",
    "width": 500,
    "height": 250,
    "autosize": {
        "resize": true,
        "contains": "padding"
    },
    "data": {
        "url": "./plots/data.json"
    },
    "mark": "line",
    "encoding": {
        "x": {
            "timeUnit": "utcmonthdate",
            "field": "date",
            "type": "temporal",
            "axis": {
                "labelAngle": 0,
                "tickCount": "day"
            }
        },
        "y": {
            "field": "valid",
            "type": "quantitative"
        }
    }
}
